{
  "homepage": ".",
  "name": "yambi_smis",
  "version": "8.4.2",
  "private": true,
  "author": {
    "name": "Joan Migani",
    "email": "joan.agisha@gmail.com"
  },
  "app_full_name": "Yambi Class - School Management Information System - (SMIS)",
  "branch": "stable",
  "platform": "Web",
  "productName": "Yambi Class SMIS",
  "main": "public/main.js",
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "@material-ui/core": "^4.12.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "convertir-nombre-lettre": "^1.0.2",
    "electron-is-dev": "^2.0.0",
    "react": "^18.2.0",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-charts": "^2.0.0-beta.7",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "read-excel-file": "^5.2.25",
    "redux": "^4.2.0",
    "web-vitals": "^2.1.4",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none yarn start\" \"yarn electron:start\"",
    "electron:build": "yarn build && electron-builder --linux -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on tcp:3000 && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "extends": null,
    "appId": "com.class.yambi",
    "linux": {
      "target": [
        "deb",
        "snap"
      ],
      "category": "Utility"
    },
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "wait-on": "^7.0.1"
  }
}
